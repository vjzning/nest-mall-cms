    "scripts": {
        "build": "nest build",
        "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
        "dev:admin": "pnpm --filter @app/web-admin dev",
        "dev:store": "pnpm --filter @app/web-store dev",
        "dev:api-admin": "nest start --watch api-admin",
        "dev:api-store": "nest start --watch api-store",
        "start:dev": "npm run dev:api-admin",
        "build:admin": "pnpm --filter @app/web-admin build",
        "build:store": "pnpm --filter @app/web-store build",
        "build:api-admin": "nest build api-admin",
        "build:api-store": "nest build api-store",
        "build:all": "pnpm build:api-admin && pnpm build:api-store && pnpm build:admin && pnpm build:store",
        "lint": "eslint \"{src,apps,packages,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./apps/api-admin/test/jest-e2e.json",
        "seed:products": "ts-node -r tsconfig-paths/register apps/api-admin/src/scripts/seed-products.ts"
    },
