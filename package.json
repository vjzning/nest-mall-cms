    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": ".",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "./coverage",
        "testEnvironment": "node",
        "roots": [
            "<rootDir>/apps/",
            "<rootDir>/packages/"
        ],
        "moduleNameMapper": {
            "^@app/db(|/.*)$": "<rootDir>/packages/db/src/$1",
            "^@app/storage(|/.*)$": "<rootDir>/packages/storage/src/$1",
            "^@app/payment(|/.*)$": "<rootDir>/packages/payment/src/$1",
            "^@app/redis(|/.*)$": "<rootDir>/packages/redis/src/$1",
            "^@app/shared(|/.*)$": "<rootDir>/packages/shared/src/$1"
        }
    }
