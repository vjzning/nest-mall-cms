{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/api-admin/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "builder": "swc",
    "tsConfigPath": "apps/api-admin/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/api-admin",
  "projects": {
    "api-admin": {
      "type": "application",
      "root": "apps/api-admin",
      "entryFile": "main",
      "sourceRoot": "apps/api-admin/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-admin/tsconfig.app.json"
      }
    },
    "api-store": {
      "type": "application",
      "root": "apps/api-store",
      "entryFile": "main",
      "sourceRoot": "apps/api-store/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-store/tsconfig.app.json"
      }
    },
    "db": {
      "type": "library",
      "root": "packages/db",
      "entryFile": "index",
      "sourceRoot": "packages/db/src",
      "compilerOptions": {
        "tsConfigPath": "packages/db/tsconfig.lib.json"
      }
    },
    "payment": {
      "type": "library",
      "root": "packages/payment",
      "entryFile": "index",
      "sourceRoot": "packages/payment/src",
      "compilerOptions": {
        "tsConfigPath": "packages/payment/tsconfig.lib.json"
      }
    },
    "redis": {
      "type": "library",
      "root": "packages/redis",
      "entryFile": "index",
      "sourceRoot": "packages/redis/src",
      "compilerOptions": {
        "tsConfigPath": "packages/redis/tsconfig.lib.json"
      }
    },
    "storage": {
      "type": "library",
      "root": "packages/storage",
      "entryFile": "index",
      "sourceRoot": "packages/storage/src",
      "compilerOptions": {
        "tsConfigPath": "packages/storage/tsconfig.lib.json"
      }
    }
  }
}